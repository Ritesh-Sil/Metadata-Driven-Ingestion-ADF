{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azrdatafactoryritesh"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ls_blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobstorage'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountritesh.dfs.core.windows.net/"
		},
		"ls_kv_s3_to_azr_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://myazrkeyvault1.vault.azure.net/"
		},
		"ls_s3_to_azr_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_kv_s3_to_azr",
					"type": "LinkedServiceReference"
				},
				"secretName": "s3-azr-integration-access-key"
			}
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azrdatafactoryritesh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"ls_blobstorage_connectionString": {
						"value": "[parameters('ls_blobstorage_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_kv_s3_to_azr_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_s3_to_azr_properties_typeProperties_baseUrl')]"
					},
					"ls_s3_to_azr_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_s3_to_azr_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}